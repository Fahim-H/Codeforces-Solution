#include <iostream>
using namespace std;
int main()
{
    int num,sum=0;
    cin>>num;
    const int index=num;
    int box[index];
    for(int i=0;i<num;i++){
        cin>>box[i];
        if(box[i]==1){
            sum+=1;
        }
        else if(box[i]==0){
            sum+=0;
        }
    }
    if(sum==0)
        cout<<"EASY"<<endl;
    if(sum>0)
        cout<<"HARD"<<endl;
    return 0;
}
