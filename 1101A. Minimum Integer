#include <iostream>
typedef long long int ll;
using namespace std;
int main(){
    ll t; cin>>t;
    while(t--){
        ll l,r,d; cin>>l>>r>>d;
        ll cpy = r+1;
        if(d<l){
            cout<<d<<endl;
        }
        else{
            ll rem = (r%d);
            cout<<(d-rem)+r<<endl;
        }
    }
    return 0;
}
