#include <iostream>
#include <algorithm>
#include <cstring>
using namespace std;
int main(){
    char first[101],second[100];
    int looper; cin>>looper;

    while(looper--){
        cin>>first; int len=strlen(first);
        cout<<first[0];
        for(int i=1;i<len;i=i+2){
            cout<<first[i];
        }
        cout<<endl;
    }
    return 0;
}
