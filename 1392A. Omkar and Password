#include <iostream>
#include <vector>
#include <algorithm>
typedef long long int ll;
using namespace std;
int main(){
    ll t; cin>>t;
    while(t--){
        ll num; cin>>num;
        vector <ll> pass(num);
        for(auto &it:pass)cin>>it;
        sort(pass.begin(),pass.end());

        if(pass.front()==pass.back())cout<<num<<endl;
        else cout<<1<<endl;
    }
    return 0;
}
