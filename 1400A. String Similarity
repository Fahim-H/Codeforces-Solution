#include <iostream>
#include <vector>
#include <algorithm>
#define FastAsRocket ios::sync_with_stdio(false);cin.tie(0);cout.tie(0)
typedef long long int ll;
using namespace std;
void production(){
    int num; string bnr;
    cin>>num>>bnr;

    string ans;
    for(int i=0; i<bnr.size(); i+=2){
        ans += bnr[i];
    }
    cout<<ans<<endl;
}
int main(){
    FastAsRocket;
    int t; cin>>t;
    while(t--){
        production();
    }
    return 0;
}
