#include <iostream>
#include <algorithm>
#include <vector>
#include <iomanip>
using namespace std;
int main(){
    vector <int> vec;
    int num,in; cin>>num;
    double ans=0.0;

    for(int i=0;i<num;i++){
        cin>>in; vec.push_back(in);
    }
    for(int i=0;i<num;i++){
        ans+=vec[i];
    }

    ans=ans/num;
    cout<<fixed<<setprecision(12)<<ans<<endl;
    return 0;
}
