#include <iostream>
#include <algorithm>
#include <string>
using namespace std;
int main(){
    ios::sync_with_stdio(0);
    cin.tie(0);

    string line,song; getline(cin,line);

    int len=line.length(),k=0;
    for(int i=0;i<len;i++){
        if(line[i]=='W' && line[i+1]=='U' && line[i+2]=='B'){
            i+=2;
            //if(k>0)
            cout<<" ";
            //k++;
        }
        else{
            cout<<line[i];
        }
    }
    cout<<endl;
    return 0;
}
