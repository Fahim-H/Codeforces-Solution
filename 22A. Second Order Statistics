#include<bits/stdc++.h>
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;


int main()
{
    optimize();
    set <int> s;
    int n; cin>>n;
    for(int i=0; i<n; i++){
        int in; cin>>in;
        s.insert(in);
    }
    if(s.size() == 1)cout<<"NO"<<endl;
    else cout<<*(++s.begin())<<endl;
    return 0;
}
