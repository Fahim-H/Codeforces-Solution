#include <iostream>
#include <cstring>
#include <string>
using namespace std;
int main()
{
    int matrix[6][6];

    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cin>>matrix[i][j];
        }
    }
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            if(matrix[i][j]==1){
                cout<<(abs(i+1-3)+abs(j+1-3))<<endl;
                break;
            }
        }
    }
    return 0;
}
