#include<bits/stdc++.h>
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;
int main()
{
    optimize();
    int n; cin>>n;
    map <string, int> m;

    while(n--){
        string name; cin>>name;
        if(m[name] == 0)cout<<"OK"<<endl;
        else cout<<name<<m[name]<<endl;
        m[name]++;
    }
    return 0;
}
