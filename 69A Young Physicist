#include <iostream>
#include<cstring>
#include <iomanip>
using namespace std;
int main()
{
    int looper,num1,num2,num3;
    int sum1=0,sum2=0,sum3=0;
    cin>>looper;
    while(looper--){
        cin>>num1>>num2>>num3;
        sum1+=num1;
        sum2+=num2;
        sum3+=num3;
    }
    if(sum1==0 && sum2==0 && sum3==0)
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
    return 0;
}
