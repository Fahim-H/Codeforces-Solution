#include<bits/stdc++.h>
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;
int main()
{
    optimize();
    int n; cin>>n;
    map <string, int> m;

    while(n--){
        string name; cin>>name;
        if(m[name] == 0)cout<<"NO"<<endl;
        else cout<<"YES"<<endl;

        m[name] = 1;
    }
    return 0;
}
